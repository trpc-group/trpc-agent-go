{
  "version": "1.0",
  "name": "transform_basic",
  "description": "Demonstrates builtin.transform reshaping user_input into a structured object.",
  "nodes": [
    {
      "id": "start",
      "label": "Start",
      "node_type": "builtin.start",
      "config": {}
    },
    {
      "id": "transform",
      "label": "Transform user_input",
      "node_type": "builtin.transform",
      "config": {
        "expr": {
          "expression": "{ \"original_text\": state.user_input, \"note\": \"This object was created by builtin.transform\" }",
          "format": "cel"
        },
        "output_schema": {
          "type": "object",
          "properties": {
            "original_text": {
              "type": "string",
              "description": "Original workflow input text"
            },
            "note": {
              "type": "string",
              "description": "Static note added by transform"
            }
          },
          "additionalProperties": false
        }
      }
    },
    {
      "id": "end",
      "label": "End",
      "node_type": "builtin.end",
      "config": {
        "expr": {
          "expression": "{ \"message\": \"Transform node has produced a structured object\", \"result\": nodes.transform.output_parsed }",
          "format": "cel"
        },
        "output_schema": {
          "type": "object",
          "properties": {
            "message": {
              "type": "string",
              "description": "Human-readable summary of the transform step"
            },
            "result": {
              "type": "object",
              "description": "Structured object produced by the transform node"
            }
          },
          "additionalProperties": false
        }
      }
    }
  ],
  "edges": [
    {
      "id": "edge_start_transform",
      "source": "start",
      "target": "transform"
    },
    {
      "id": "edge_transform_end",
      "source": "transform",
      "target": "end"
    }
  ],
  "conditional_edges": [],
  "start_node_id": "start"
}
